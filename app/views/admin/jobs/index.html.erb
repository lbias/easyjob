<h1>全部的职位在此</h1>

<table class="table">
  <% @jobs.each do |job| %>
  <tr>
    <td><%= link_to job.cp_name, job_path(job) %></td>
    <td><%= job.title %></td>
    <td><%= job.wage_lower_bound %>
      -
      <%= job.wage_upper_bound %></td>
    <td><%= link_to("Edit", edit_admin_job_path(job), class: "btn btn-primary") %></td>
    <td><%= link_to("Delete", admin_job_path(job), method: :delete, class: "btn btn-primary", data: {comfirm: "Are you sure?"}) %></td>
  </tr>
  <% end  %>
</table>
